{
  "user_options": {
    "extender": {
      "python": { "location_of_jython_standalone_jar_file": "{{JYTHON_PATH}}" },
      "ruby": { "location_of_jruby_jar_file": "{{JRUBY_PATH}}" }
    },
    "misc": {
      "pause_tasks_at_startup_default": false,
      "show_learn_tab": false
    },
    "proxy": {
      "http_history": {
        "sort_column": "#",
        "sort_order": "descending"
      },
      "websockets_history": {
        "sort_column": "#",
        "sort_order": "descending"
      }
    }
  }
}
